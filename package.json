{
  "name": "fse",
  "version": "2.0.0",
  "description": "fs: then-ified, extended, and dependency-free",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "jonathantneal/fse",
  "homepage": "https://github.com/jonathantneal/fse#readme",
  "bugs": "https://github.com/jonathantneal/fse/issues",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "prepublish": "npm test",
    "test": "echo 'Running tests...'; npm run test:js && npm run test:node",
    "test:js": "eslint *.js --cache --ignore-pattern .gitignore",
    "test:node": "node test"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-dev": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "dev",
    "rules": {
      "no-extra-parens": [0],
      "no-nested-ternary": [0]
    }
  },
  "keywords": [
    "fs",
    "promise",
    "extended"
  ]
}
